def puntaje(a):
    dic_puntajes={}
    values = []
    puntaje=0
    valor = 0
    for i in (a):
        values = a[i]
        for j in range (len(values)):
            if j == 0:
                if str(values[0]).upper() == "SI":
                    puntaje+=10
            if j == 1:
                if str(values[1]).upper() == "SI":
                    puntaje+=10
