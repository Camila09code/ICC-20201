                valor = int(values[26])
                puntaje += (valor*2)
            if j == 27:
                valor = int(values[27])
                puntaje += (valor*1)
            if j == 28:
                valor = int(values[28])
                puntaje += (valor*2)
        values = []
        dic_puntajes[i] = puntaje
        puntaje=0
    return dic_puntajes
from CSV_a_Python import dic_ipress
